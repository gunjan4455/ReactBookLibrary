{
  "name": "react_book_express",
  "version": "0.1.0",
  "description": "A demo for running a Webpack workflow in Node express with ReactJS",
  "main": "server.js",
  "keywords": [
    "express",
    "webpack",
    "node"
  ],
  "author": "Gunjan Jain",
  "scripts": {
    "test": "",
    "start": "node server",
    "prod": "webpack --config webpack.production.config.js",
    "build": "rimraf dist && cross-env NODE_ENV=production webpack --config ./webpack.production.config.js --progress --profile --colors",
    "build:prod": "npm-run-all --parallel prod start",
    "eslint": "eslint .",
    "jscs": "jscs ."
  },
  "dependencies": {
    "babel-cli": "^6.4.0",
    "babel-core": "^6.22.1",
